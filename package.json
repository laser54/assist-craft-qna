{
  "name": "assist-craft-qna",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "server"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:frontend",
    "dev:stack": "npm run dev",
    "dev:server": "npm run dev --workspace server",
    "dev:frontend": "cross-env VITE_API_BASE=/api npm run dev --workspace frontend"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
